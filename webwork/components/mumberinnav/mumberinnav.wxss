/* 会员中心内标头样式 */
/* 顶部菜单分割线 */
.linenav{
  width: 100%;
  height: 20rpx;
  background-color:rgba(0,0,0,0.1);
}
/* 内容的表头样式 */
.content-nav{
  display: flex;
}
.content-navtext{
  font-size: 28rpx;
  color: rgb(51, 51, 51);
  flex: 7;
  padding-left: 24rpx;
  padding-top: 28rpx;
  padding-bottom: 28rpx;
}
/* 分隔线样式 */
.linebottom{
  border-top: 2rpx solid rgb(224, 224, 224);
}

